    
    

class Complex
{
    private:
        int re;
        int im;
    public:
        Complex()
        {
            re = 0;
            im = 0;
            printf("constructor default\n");
        }

        Complex(const Complex& c)
        {
            re = c.re;
            im = c.im;
            printf("copy contructor\n");
        }

        void operator=(const Complex& c)
        {
            re = c.re;
            im = c.im;
            printf("assignment operator\n");
        }
        ~Complex ()
        {
            printf("destructor\n");
         }
};