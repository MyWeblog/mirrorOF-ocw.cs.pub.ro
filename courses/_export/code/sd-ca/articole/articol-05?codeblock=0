    
    

template <typename T, int N>
class Queue {
private:
	int head;
	int tail;
	int size;
	T queueArray[N];

public:
	// Constructor
	Queue();

	// Destructor
	~Queue();

	// Adauga la coada
	void enqueue(T e);

	// Extrage din coada
	T dequeue();

	// Afla primul element
	T front() ;

	bool isEmpty();
};