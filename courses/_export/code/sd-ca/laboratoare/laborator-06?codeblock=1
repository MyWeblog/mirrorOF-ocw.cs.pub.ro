#define __STACK__H

template<typename T>
class Stack {
public:
    // Constructor
    Stack();

    // Destructor
    ~Stack();

    void push(T x);
    T pop();
    T peek();
    int isEmpty();

private:
    // Vectorul de stocare.
    LinkedList<T> stackList;
    // De ce nu mai este nevoie sa retinem topLevel?
};
#endif